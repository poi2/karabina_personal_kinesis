{
  "title": "KINESIS",
  "rules": [
    {
      "description": "Kinesis: us-backslash to ~",
      "manipulators": [
        {
          "from": {
            "key_code": "non_us_backslash"
          },
          "to": [
            {
              "key_code": "grave_accent_and_tilde",
              "modifiers": "left_shift"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Kinesis: Home to Escape + japanese_eisuu",
      "manipulators": [
        {
          "from": {
            "key_code": "home"
          },
          "to": [
            {
              "key_code": "escape"
            },
            {
              "key_code": "japanese_eisuu"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Kinesis: End to Ctrl + Alt + <-. (Move to left tab)",
      "manipulators": [
        {
          "from": {
            "key_code": "end"
          },
          "to": {
            "key_code": "left_arrow",
            "modifiers": [
              "left_command",
              "left_option"
            ]
          },
          "type": "basic"
        }
      ]
    }
  ]
}
